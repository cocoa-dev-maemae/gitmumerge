usage="Usage: gitmumerge -d(--destination-branch) branch name -c(--candidate-branches) branch name1 name2..."
d_option="-d,--destination-branch branch name\t\t Specify destination branch"
c_option="-c,--candidate-branches branch name1 name2...\t Specify candidate branches"
h_option="-h,--help\t\t\t\t\t Show help"
v_option="-v,--version\t\t\t\t\t Show version"
echo -e $usage
echo "options:"
echo -e $d_option
echo -e $c_option
echo -e $h_option
echo -e $v_option
exit 0
